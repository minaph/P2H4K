{
  "configuration": {
    "config": {
      "configName": "config",
      "topScope": [
        "const path = require('path');",
        "const webpack = require('webpack');",
        "\n"
      ],
      "webpackOptions": {
        "mode": "'development'",
        "entry": {
          "main": "'./src/main.ts'"
        },
        "plugins": [
          "new webpack.ProgressPlugin()"
        ],
        "module": {
          "rules": [
            {
              "test": "/\\.(ts|tsx)$/",
              "loader": "'ts-loader'",
              "include": [
                "path.resolve(__dirname, 'src')"
              ],
              "exclude": [
                "/node_modules/"
              ]
            }
          ]
        },
        "resolve": {
          "extensions": [
            "'.tsx'",
            "'.ts'",
            "'.js'"
          ]
        },
        "devServer": {
          "open": true,
          "host": "'localhost'"
        }
      }
    }
  },
